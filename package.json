{
    "name": "@vrbo/fpsmeter",
    "version": "1.0.0",
    "description": "Small utility that measures frames per second in a browser context.",
    "license": "Apache-2.0",
    "author": {
        "name": "Expedia Group",
        "email": "oss@expediagroup.com"
    },
    "contributors": [
        "neenhouse",
        "kashorn",
        "brianjquinn",
        "cmoroexpedia"
    ],
    "main": "lib/transpiled/index.js",
    "module": "lib/esm/index.js",
    "sideEffects": [
        "*.css",
        "*.less"
    ],
    "scripts": {
        "start": "webpack-dev-server --open",
        "build": "npm run build:modern && npm run build:esm && npm run build:umd",
        "build:docs": "webpack",
        "build:esm": "BABEL_ENV=esm babel src --out-dir lib/esm --source-maps",
        "build:modern": "BABEL_ENV=modern babel src --out-dir lib/transpiled --source-maps",
        "build:umd": "BABEL_ENV=umd webpack --bail",
        "build:analyze": "NODE_ENV=production BABEL_ENV=umd webpack",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "test": "npm run test:lint && npm run test:unit",
        "test:lint": "eslint --color src",
        "test:unit": "jest --coverage",
        "test:unit:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --colors --detectOpenHandles",
        "semantic-release": "semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/expediagroup/fpsmeter.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "bugs": {
        "url": "https://github.com/expediagroup/fpsmeter/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "nyc": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100,
        "exclude": [
            "setup.js",
            "tests/**/*.js"
        ],
        "colors": true,
        "reporter": [
            "text",
            "text-summary",
            "html"
        ],
        "instrument": false,
        "sourceMap": false
    },
    "engines": {
        "node": ">=10.0.0",
        "npm": ">=6.0.0"
    },
    "keywords": [
        "fpsmeter",
        "fps",
        "meter"
    ],
    "dependencies": {},
    "peerDependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-transform-destructuring": "^7.5.0",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.0.0",
        "autoprefixer": "^7.1.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.0",
        "babel-plugin-istanbul": "^5.2.0",
        "babel-plugin-transform-rename-import": "^2.3.0",
        "chai": "^4.1.2",
        "classnames": "^2.0.0",
        "coveralls": "^3.0.6",
        "css-loader": "^1.0.0",
        "cssnano": "^4.1.4",
        "eslint": "^8.0.0",
        "eslint-config-standard": "^13.0.1",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-standard": "^4.0.0",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "ignore-styles": "^5.0.1",
        "jest": "^25.1.0",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "less-plugin-npm-import": "^2.1.0",
        "mini-css-extract-plugin": "^0.5.0",
        "open": "7.0.0",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "prop-types": "^15.7.2",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-responsive-carousel": "^3.1.49",
        "react-test-renderer": "^16.0.0",
        "semantic-release": "^17.0.4",
        "style-loader": "^0.23.1",
        "system-bell-webpack-plugin": "^1.0.0",
        "terser-webpack-plugin": "^1.1.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.21.0",
        "webpack-bundle-analyzer": "^3.1.0",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.7",
        "webpack-merge": "^4.1.4",
        "webpack-module-analyzer-plugin": "^0.3.0",
        "webpack-visualizer-plugin": "^0.1.11"
    }
}
